		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svg-sprite": "^2.0.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"@codemirror/autocomplete": "^6.9.0",
		"@codemirror/commands": "^6.2.4",
		"@codemirror/lang-javascript": "^6.1.9",
		"@codemirror/language": "^6.8.0",
